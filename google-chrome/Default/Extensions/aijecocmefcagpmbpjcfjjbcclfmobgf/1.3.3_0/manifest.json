{
   "action": {
      "default_popup": "popup.html",
      "default_title": "SparX Wallet"
   },
   "background": {
      "service_worker": "js/worker.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/contentscript.js" ],
      "matches": [ "file://*/*", "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
   },
   "description": "SparX Wallet: your all-in-one tool for managing crypto assets via TVM networks.",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "256": "icon256.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "minimum_chrome_version": "102",
   "name": "SparX Wallet",
   "permissions": [ "storage", "unlimitedStorage", "clipboardWrite", "activeTab", "notifications", "alarms" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.3.3",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "/js/*", "/assets/*", "phishing-warning.html" ]
   } ]
}
