   "description": "__MSG_extDesc__",
   "host_permissions": [ "*://*.localhost/*", "*://*.aliradar.com/*", "*://*.aliexpress.com/*", "*://*.aliexpress.ru/*", "*://*.flipkart.com/*", "*://*.gearbest.com/*", "*://*.banggood.com/*", "*://*.joom.com/*", "*://*.mvideo.ru/*", "*://*.beru.ru/*", "*://*.amazon.com/*", "*://*.ebay.com/*", "*://*.dns-shop.ru/*", "*://*.citilink.ru/*", "*://*.asos.com/*", "*://*.alibaba.com/*", "*://*.eldorado.ru/*", "*://*.vseinstrumenti.ru/*", "*://*.goods.ru/*", "*://*.tmall.ru/*", "*://*.tokopedia.com/*", "*://*.market.yandex.ru/*" ],
   "icons": {
      "128": "assets/img/logo/96.png",
      "16": "assets/img/logo/96.png",
      "48": "assets/img/logo/96.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "__MSG_extName__",
   "options_page": "settings.html",
   "permissions": [ "cookies", "storage", "declarativeNetRequest", "declarativeNetRequestFeedback", "tabs", "unlimitedStorage", "alarms" ],
   "sandbox": {
      "pages": [ "sandbox.html" ]
   },
   "short_name": "__MSG_extSname__",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.0.0",
   "web_accessible_resources": [ {
      "matches": [ "*://*/*" ],
      "resources": [ "assets/*/*", "_locales/*/*", "settings.html" ]
   } ]
}
