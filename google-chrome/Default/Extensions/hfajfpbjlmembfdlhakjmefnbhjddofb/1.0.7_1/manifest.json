{
   "action": {
      "default_popup": "index.html",
      "default_title": "ION Wallet"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content.js" ],
      "matches": [ "file://*/*", "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "description": "ION Wallet in your Browser",
   "icons": {
      "128": "ion-logo128.png",
      "16": "ion-logo16.png",
      "48": "ion-logo48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "ION Wallet",
   "permissions": [ "proxy", "storage", "unlimitedStorage", "clipboardWrite", "activeTab" ],
   "short_name": "ION Wallet",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.7",
   "web_accessible_resources": [ {
      "matches": [ "file://*/*", "http://*/*", "https://*/*" ],
      "resources": [ "provider.js" ]
   } ]
}
